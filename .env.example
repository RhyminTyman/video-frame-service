# Server
PORT=3001
CORS_ORIGIN=http://localhost:3000

# Security
# Comma-separated list of API keys. Rotate by adding a new key and removing old.
API_KEYS=dev_key_123
# Optional HMAC signing: comma-separated list of keyId:secret
SIGNING_KEYS=keyid1:supersecret1
SIGNING_MAX_SKEW=300

# Multer limits
# Max accepted upload bytes (e.g., 200MB = 209715200). Must be >= the videos you'll accept.
MAX_UPLOAD_BYTES=209715200

# AWS
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=YOUR_ACCESS_KEY_ID
AWS_SECRET_ACCESS_KEY=YOUR_SECRET_ACCESS_KEY
S3_BUCKET=your-bucket-name
S3_PREFIX=frames
